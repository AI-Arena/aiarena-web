{
  "listeners": {
    "*:8301": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "uri": "/static/*"
      },
      "action": {
        "share": "/app/$uri"
      }
    },
    {
      "match": {
        "uri": "/health-check/"
      },
      "action": {
        "pass": "applications/django",
        "response_headers": {
          "Host": "aiarena-test.net",
          "X-Forwarded-Proto": "https"
        }
      }
    },
    {
      "action": {
        "pass": "applications/django"
      }
    }
  ],
  "applications": {
    "django": {
      "type": "python",
      "path": "/app/",
      "module": "aiarena.wsgi"
    }
  },
  "settings": {
    "http": {
      "max_body_size": 209715200
    }
  }
}